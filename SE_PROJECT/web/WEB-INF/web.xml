<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.jis.judiciary.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CaseServlet</servlet-name>
        <servlet-class>com.jis.judiciary.CaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAccountServlet</servlet-name>
        <servlet-class>com.jis.judiciary.CreateAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetrieveUsersServlet</servlet-name>
        <servlet-class>com.jis.judiciary.RetrieveUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetriveUsersServlet</servlet-name>
        <servlet-class>com.jis.judiciary.RetriveUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCaseServlet</servlet-name>
        <servlet-class>com.jis.judiciary.UpdateCaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCaseServlet</servlet-name>
        <servlet-class>com.jis.judiciary.DeleteCaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CaseServlet</servlet-name>
        <url-pattern>/CaseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAccountServlet</servlet-name>
        <url-pattern>/CreateAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetrieveUsersServlet</servlet-name>
        <url-pattern>/RetrieveUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetriveUsersServlet</servlet-name>
        <url-pattern>/RetriveUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateCaseServlet</servlet-name>
        <url-pattern>/UpdateCaseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCaseServlet</servlet-name>
        <url-pattern>/DeleteCaseServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
